{
  "version": "1.0.9",
  "engines": {
    "node": ">=14.16"
  },
  "author": "Steve Lebleu",
  "license": "MIT",
  "name": "ts-express-typeorm-boilerplate",
  "description": "This repository contains REST API boilerplate Typescript / Express.js / TypeORM based.",
  "keywords": [
    "node",
    "express.js",
    "typescript",
    "typeorm",
    "mysql",
    "boilerplate",
    "api",
    "rest",
    "restful",
    "es2017"
  ],
  "homepage": "https://github.com/konfer-be/ts-express-typeorm-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konfer-be/ts-express-typeorm-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/konfer-be/ts-express-typeorm-boilerplate/issues"
  },
  "main": "./dist/api/app.bootstrap.js",
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "scripts": {
    "kickstart": "npm i typescript -g && npm i typeorm -g && npm i kfr-kem -g && npm i && npm-run-all -s kick:*",
    "kick:directories": "mkdir -p ./dist/logs ./dist/public/archives ./dist/public/documents ./dist/public/images/master-copy ./dist/public/images/rescale/xs ./dist/public/images/rescale/sm ./dist/public/images/rescale/md ./dist/public/images/rescale/lg ./dist/public/images/rescale/xl ./dist/public/audios ./dist/public/videos",
    "kick:env": "cp -r ./src/env/template.env ./dist/env/development.env && cp -r ./src/env/template.env ./dist/env/test.env && cp -r ./src/env/template.env ./dist/env/stagin.env && cp -r ./src/env/template.env ./dist/env/production.env",
    "lint": "eslint ./src/**/*.ts",
    "test": "nyc --reporter=html --report-dir=./docs/nyc-coverage ./node_modules/.bin/mocha --exit --reporter spec --timeout 10000 --env test",
    "coverage": "nyc --reporter=lcov --report-dir=./docs/coverage ./node_modules/.bin/mocha --exit --reporter spec --timeout 10000 && cat ./docs/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./docs/coverage",
    "todo-ci": "leasot -x --reporter markdown './src/**/*.ts' > TODO.md",
    "apidoc-ci": "apidoc -i src/ -o docs/apidoc/",
    "typedoc-ci": "typedoc --mode modules --name 'API documentation' --defaultCategory 'Root' --categorizeByGroup  --out ./docs/typedoc ./src",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "_moduleAliases": {
    "@bases": "dist/api/types/classes",
    "@config": "dist/api/config",
    "@controllers": "dist/api/controllers",
    "@decorators": "dist/api/decorators",
    "@enums": "dist/api/types/enums",
    "@errors": "dist/api/types/errors",
    "@events": "dist/api/events",
    "@factories": "dist/api/factories",
    "@interfaces": "dist/api/types/interfaces",
    "@middlewares": "dist/api/middlewares",
    "@models": "dist/api/models",
    "@repositories": "dist/api/repositories",
    "@routes": "dist/api/routes/v1",
    "@serializers": "dist/api/serializers",
    "@services": "dist/api/services",
    "@utils": "dist/api/utils",
    "@validations": "dist/api/validations",
    "@whitelists": "dist/api/serializers/whitelists"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/node": "^12.7.2",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.18.3",
    "boom": "^7.3.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "es6-promisify": "^6.0.1",
    "express": "^4.16.4",
    "express-rate-limit": "^5.0.0",
    "express-validation": "^1.0.2",
    "express-validator": "^6.1.1",
    "filenamify": "^4.1.0",
    "helmet": "^3.23.3",
    "hpp": "^0.2.2",
    "http-status": "^1.3.1",
    "jimp": "^0.14.0",
    "joi": "^14.3.1",
    "jsonapi-serializer": "^3.6.4",
    "jwt-simple": "^0.5.5",
    "lodash": "^4.17.19",
    "module-alias": "^2.2.1",
    "moment-timezone": "^0.5.23",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "mysql": "^2.18.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.31",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "apidoc": "^0.17.7",
    "auto-changelog": "^2.2.1",
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "coveralls": "^3.1.0",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^5.1.3",
    "leasot": "^8.0.0",
    "mocha": "^8.3.0",
    "nock": "^10.0.6",
    "node-notifier": "^8.0.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "typedoc": "^0.15.0",
    "typescript": "^4.2.2",
    "typescript-eslint": "0.0.1-alpha.0"
  }
}
