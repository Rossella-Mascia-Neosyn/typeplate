{
  "version": "1.2.2",
  "engines": {
    "node": ">=14.16"
  },
  "author": "Steve Lebleu",
  "license": "MIT",
  "name": "ts-express-typeorm-boilerplate",
  "description": "REST API boilerplate builded with Typescript, Express.js, TypeORM and Mocha.",
  "keywords": [
    "node",
    "express.js",
    "typescript",
    "typeorm",
    "mysql",
    "boilerplate",
    "api",
    "rest",
    "restful",
    "es2017",
    "mocha"
  ],
  "homepage": "https://github.com/konfer-be/ts-express-typeorm-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konfer-be/ts-express-typeorm-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/konfer-be/ts-express-typeorm-boilerplate/issues"
  },
  "main": "./dist/api/app.bootstrap.js",
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": false,
    "commitLimit": false
  },
  "scripts": {
    "build": "npm-run-all -s build:*",
    "build:directories": "mkdir -p ./dist/logs ./dist/public/archives ./dist/public/documents ./dist/public/images/master-copy ./dist/public/images/rescale/xs ./dist/public/images/rescale/sm ./dist/public/images/rescale/md ./dist/public/images/rescale/lg ./dist/public/images/rescale/xl ./dist/public/audios ./dist/public/videos",
    "build:env": "cp -r ./src/env/template.env ./dist/env/development.env && cp -r ./src/env/template.env ./dist/env/test.env && cp -r ./src/env/template.env ./dist/env/stagin.env && cp -r ./src/env/template.env ./dist/env/production.env",
    "ci:coverage": "nyc --reporter=lcov --report-dir=./reports/coverage npm-run-all -s test:* && cat ./reports/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coverage",
    "ci:test": "npm-run-all -s test:*",
    "doc": "npm-run-all -s doc:*",
    "doc:apidoc": "apidoc -i src/ -o docs/apidoc/",
    "kickstart": "npm i typescript@4.2.2 -g && npm i typeorm@0.2.31 -g && npm i kfr-kem -g && npm i && npm run build && npm run doc",
    "test": "nyc --reporter=html --report-dir=./reports/nyc-coverage npm-run-all -s test:*",
    "test:unit": "./node_modules/.bin/mocha ./test/units/00-application.unit.test.js --exit --reporter spec --timeout 10000 --env test",
    "test:e2e": "./node_modules/.bin/mocha ./test/e2e/00-api.e2e.test.js --exit --reporter spec --timeout 10000 --env test",
    "todo": "leasot -x --reporter markdown './src/**/*.ts' > TODO.md && git add TODO.md",
    "version": "npm run todo && git add TODO.md && git add package.json && auto-changelog -p && git add CHANGELOG.md && git commit -m \"Update changelog\" --no-verify"
  },
  "_moduleAliases": {
    "@bases": "dist/api/types/classes",
    "@config": "dist/api/config",
    "@controllers": "dist/api/controllers",
    "@customtypes": "dist/api/types/types",
    "@decorators": "dist/api/decorators",
    "@enums": "dist/api/types/enums",
    "@errors": "dist/api/types/errors",
    "@factories": "dist/api/factories",
    "@interfaces": "dist/api/types/interfaces",
    "@middlewares": "dist/api/middlewares",
    "@models": "dist/api/models",
    "@repositories": "dist/api/repositories",
    "@routes": "dist/api/routes/v1",
    "@schemas": "dist/api/validations/schemas",
    "@services": "dist/api/services",
    "@utils": "dist/api/utils",
    "@validations": "dist/api/validations"
  },
  "dependencies": {
    "@hapi/boom": "9.1.1",
    "axios": "0.21.1",
    "bcrypt": "5.0.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "es6-promisify": "6.1.1",
    "express": "4.16.4",
    "express-rate-limit": "5.2.6",
    "filenamify": "4.2.0",
    "helmet": "4.4.1",
    "hpp": "0.2.3",
    "http-status": "1.5.0",
    "jimp": "0.16.1",
    "joi": "17.4.0",
    "jwt-simple": "0.5.6",
    "lodash": "4.17.21",
    "memory-cache": "0.2.0",
    "module-alias": "2.2.2",
    "moment-timezone": "0.5.33",
    "morgan": "1.10.0",
    "multer": "1.4.2",
    "mysql": "2.18.1",
    "passport": "0.4.1",
    "passport-facebook": "3.0.0",
    "passport-github2": "0.1.12",
    "passport-google-oauth": "2.0.0",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.0",
    "passport-linkedin-oauth2": "2.0.0",
    "pluralize": "8.0.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.31",
    "uuid": "8.3.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/body-parser": "1.19.0",
    "@types/express": "4.16.0",
    "@types/node": "12.7.2",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "apidoc": "0.26.0",
    "auto-changelog": "2.2.1",
    "chai": "4.3.4",
    "chance": "1.1.7",
    "coveralls": "3.1.0",
    "eslint": "7.21.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.2.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "5.1.3",
    "leasot": "11.5.0",
    "mocha": "8.3.0",
    "nock": "10.0.6",
    "node-notifier": "8.0.1",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "sinon": "9.2.4",
    "supertest": "6.1.3",
    "typescript": "4.2.2",
    "typescript-eslint": "0.0.1-alpha.0"
  }
}
